{
    "name": "stonehilt/bootstrap",
    "version": "12.0.0",
    "description": "StoneHilt Component integration for Bootstrap CSS library",
    "minimum-stability": "stable",
    "license": "proprietary",
    "keywords": ["Laravel", "StoneHilt", "Bootstrap"],
    "authors": [
        {
            "name": "Anthony Green",
            "email": "bootstrap@stonehilt.com",
            "homepage": "https://stonehilt.com"
        }
    ],
    "require": {
        "php": "^8.4",
        "ext-json": "*",
        "laravel/framework": "~12",
        "symfony/http-foundation": "^7.2.0",
        "stonehilt/blade": "~12"
    },
    "require-dev": {
        "avastechnology/iolaus": "*",
      "fakerphp/faker": "^1.24",
      "phpunit/phpunit": "^10.5.35|^11.5.3|^12.0.1"
    },
    "autoload": {
        "psr-4": {
            "StoneHilt\\Bootstrap\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "StoneHilt\\Bootstrap\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "StoneHilt\\Bootstrap\\StoneHiltBootstrapServiceProvider"
            ]
        }
    },
    "suggest": {
        "twbs/bootstrap": "This is the bootstrap CSS library that is needed when hosting all files locally."
    }
}
